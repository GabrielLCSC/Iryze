# Supabase Configuration Example
# Copy this to supabase/config.toml and adjust for your project

[project]
id = "your-project-ref"

[api]
enabled = true
port = 54321
schemas = ["public", "auth"]
extra_search_path = ["public", "extensions"]
max_rows = 1000

[db]
port = 54322
major_version = 15

[studio]
enabled = true
port = 54323

[functions]
scan = { verify_jwt = false }
